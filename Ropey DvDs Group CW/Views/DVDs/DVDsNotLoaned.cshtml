
@{
    ViewData["Title"] = "DVDsNotLoaned";
}

<h1 class="text-center mb-3">DVDs Not Loaned</h1>
<h4 class="mt-1 mb-3">Displaying a list of all DVD titles in the shop where no copy of the title has been loaned in the last 31 days</h4>

<table class="table">
    <thead>
        <tr>
            <th>
                Copy Number
            </th>
            <th>
                DVD Title
            </th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
        <td>@item.CopyNumber</td>
        <td>@item.Title</td>

        </tr>
}
    </tbody>
</table>