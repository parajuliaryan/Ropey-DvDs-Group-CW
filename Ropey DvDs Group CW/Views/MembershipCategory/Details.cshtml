    ViewData["Title"] = "Details";
}

<div>
    <h4>Membership Category Details</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            Membership Category Description
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.MembershipCategoryDescription)
        </dd>
